    "@types/react": "16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-grid-layout": "^0.16.5",
    "@types/react-plotly.js": "^2.5.0",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-virtualized": "^9.18.11",
    "@types/uuid": "^3.4.4",
    "@types/webpack-env": "^1.13.6",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
