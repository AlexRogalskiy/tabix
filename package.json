{
  "name": "tabix",
  "version": "21.12.0",
  "description": "Tabix",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "Apache-2.0",
  "homepage": "https://tabix.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/tabixio/tabix.git"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "start": "yarn run clean && cross-env NODE_ENV=development && webpack-dev-server --mode development --color --progress --config webpack.config.babel.js",
    "profile": "yarn run clean && cross-env NODE_ENV=production webpack --profile --json --config webpack.config.babel.js > ./dist/profile.json && webpack-bundle-analyzer ./dist/profile.json",
    "build:ts": "yarn run clean && tsc --project ./app/tsconfig.json",
    "build:dev": "yarn run clean && webpack --config webpack.config.babel.js",
    "build:prod": "yarn run clean && cross-env NODE_ENV=production && webpack-dev-server --mode production --color --progress --config webpack.config.babel.js"
  },
  "resolutions": {
    "**/**/fsevents": "^1.2.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.(css|less|scss)": [
      "stylelint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
